  <!-- Terraformation -->

  <!-- AtolltoEden -->

  <PlanetTerraformationDefinition Name="PlanetTerraformationFromAtollToCustom11" SubCategory="PlanetTerraformationDust">
    <AIGain RessourceName="FoodGain"/>
    <AIGain RessourceName="IndustryGain"/>

    <SupervisorGain Name="Food"     Weight="2"/>
    <SupervisorGain Name="Industry" Weight="4"/>
    <SupervisorGain Name="Science"  Weight="2"/>
    <SupervisorGain Name="Dust"     Weight="2"/>
    <SupervisorGain Name="Influence"     Weight="1"/>
    <SupervisorGain Name="Approval" Weight="1"/>

    <CustomCost ResourceName="SystemProduction">5500 * Property(StockLocation, @../ClassEmpire, TerraformationModifier)</CustomCost>
    <CustomCost ResourceName="Strategic1" Instant="true">60</CustomCost>
    <CustomCost ResourceName="Strategic2" Instant="true">60</CustomCost>
    <CustomCost ResourceName="Strategic3" Instant="true">10</CustomCost>
    <CustomCost ResourceName="Strategic4" Instant="true">10</CustomCost>
    <CustomCost ResourceName="Strategic5" Instant="true">3</CustomCost>
    <CustomCost ResourceName="Strategic6" Instant="true">3</CustomCost>
    <CustomCost ResourceName="EmpireManpower" Instant="true">1000</CustomCost>
    <!--
    <CustomCost ResourceName="NetSystemInfluence" Instant="true">100</CustomCost>
    -->


    <TechnologyPrerequisite UnlockHidden="true" Flags="Technology,Prerequisite,Discard">TechnologyTerraformation2</TechnologyPrerequisite>
    <PathPrerequisite Flags="Prerequisite,Discard">PlanetTypeAtoll</PathPrerequisite>

    <SimulationDescriptorReference Name="PlanetTypeCustom11"/>
    <SimulationDescriptorReference Name="PlanetGameplayTypeTemperate"/>
    <SimulationDescriptorReference Name="PlanetGameplayTypeCold"/>
    <SimulationDescriptorReference Name="PlanetGameplayTypeHot"/>
    <SimulationDescriptorReference Name="PlanetGameplayTypeTeeming"/>
    <SimulationDescriptorReference Name="PlanetGameplayTypeTree"/>

    <RemovedType>PlanetType</RemovedType>
    <RemovedType>PlanetGameplayType</RemovedType>
  </PlanetTerraformationDefinition>


