    <SimulationDescriptor Name="SpecialNodeEffectSolarNebula1" Type="SpecialNodeEffect" IsSerializable="false">
        <Modifier TargetProperty="SystemMoney" Operation="Addition" Value="200" />
		<Modifier TargetProperty="Strategic2"        Value="5" Operation="Addition"  />
		<Modifier TargetProperty="Strategic1"        Value="1" Operation="Addition"  />		
    </SimulationDescriptor>
	
    <SimulationDescriptor Name="SpecialNodeEffectSolarNebula2" Type="SpecialNodeEffect" IsSerializable="false">
