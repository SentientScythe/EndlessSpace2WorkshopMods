    </SimulationDescriptor>	

	<!-- Neutron Star -->

    <SimulationDescriptor Name="SpecialNodeEffectNeutronStar1" Type="SpecialNodeEffect" IsSerializable="false">
		<BinaryModifier TargetProperty="SystemResearch"	Operation="Addition" Left="10" BinaryOperation="Multiplication" Right="$(SystemPopulation)" />
		<BinaryModifier TargetProperty="SystemEmpirePoint"	Operation="Addition" Left="5" BinaryOperation="Multiplication" Right="$(SystemPopulation)" />
		<Modifier TargetProperty="Strategic5"        Value="2" Operation="Addition"  />
		<Modifier TargetProperty="Strategic4"        Value="2" Operation="Addition"  />
		<Modifier TargetProperty="Strategic6"        Value="3" Operation="Addition"  />
	</SimulationDescriptor>
	
    <SimulationDescriptor Name="SpecialNodeEffectNeutronStar2" Type="SpecialNodeEffect" IsSerializable="false">
		<BinaryModifier TargetProperty="SystemResearch"	Operation="Addition" Left="10" BinaryOperation="Multiplication" Right="$(SystemPopulation)" />
		<BinaryModifier TargetProperty="SystemEmpirePoint"	Operation="Addition" Left="5" BinaryOperation="Multiplication" Right="$(SystemPopulation)" />
		<Modifier TargetProperty="Strategic5"        Value="2" Operation="Addition"  />
		<Modifier TargetProperty="Strategic4"        Value="1" Operation="Addition"  />
		<Modifier TargetProperty="Strategic6"        Value="4" Operation="Addition"  />	
	</SimulationDescriptor>	



	<!-- Space Clouds -->

    <SimulationDescriptor Name="SpecialNodeEffectSpaceClouds" Type="SpecialNodeEffect" IsSerializable="false">
		<BinaryModifier TargetProperty="SystemMoney"	Operation="Addition" Left="10" BinaryOperation="Multiplication" Right="$(SystemPopulation)" />
		<BinaryModifier TargetProperty="SystemResearch"	Operation="Addition" Left="10" BinaryOperation="Multiplication" Right="$(SystemPopulation)" />
        <BinaryModifier TargetProperty="SystemProduction"	Operation="Addition" Left="10" BinaryOperation="Multiplication" Right="$(SystemPopulation)" />
		<BinaryModifier TargetProperty="SystemEmpirePoint"	Operation="Addition" Left="2" BinaryOperation="Multiplication" Right="$(SystemPopulation)" />
		<Modifier TargetProperty="Strategic1"        Value="1" Operation="Addition"  />
		<Modifier TargetProperty="Strategic2"        Value="1" Operation="Addition"  />
		<Modifier TargetProperty="Strategic3"        Value="1" Operation="Addition"  />
		<Modifier TargetProperty="Strategic5"        Value="1" Operation="Addition"  />
    </SimulationDescriptor>
	
	
	<!-- Vaulters -->
	<SimulationDescriptor Name="SpecialNodeEffectVaulters" Type="SpecialNodeEffect" IsSerializable="false">
        <Modifier TargetProperty="SystemMoney" Operation="Addition" Value="250" />
		<Modifier TargetProperty="Strategic4"        Value="1" Operation="Addition"  />
		<Modifier TargetProperty="Strategic6"        Value="1" Operation="Addition"  />
        <Modifier TargetProperty="HealthRegenerationRatio" Operation="Addition" Value="0.2" TooltipHidden="true"/>
        <Modifier TargetProperty="DockedFleetsHealthRegenerationRatio" Operation="Addition" Value="0.2"  TooltipOverride="%StarSystemImprovementUniqueCuriosity3EffectOverride"/>
    </SimulationDescriptor>
    
    
</Datatable>