    <StarSystemImprovementDefinition Name="StarSystemImprovementUniqueStrategicVaulters"   Category="CategoryStarSystemImprovementUnique" SubCategory="SubCategoryIndustry" ScoreProvider="StarSystemImprovementUniqueBuilt">
        <AIGain RessourceName="ResourceBonus">
            <Tag>ResourceTypeStrategic</Tag>
        </AIGain>
        <SupervisorGain Name="Industry" Weight="15"  />
        <AIGain RessourceName="IndustryGain" />			
        <QueuedSimulationDescriptorReference Name="BuildingStarSystemImprovementUniqueStrategicVaulters"/>
        <Cost ResourceName="SystemProduction">1120</Cost>
        <Cost ResourceName="EmpireManpower" Instant="true">50</Cost>
        <PathPrerequisite                           Flags="Prerequisite,Discard">ColonizedStarSystemStateColony</PathPrerequisite>
        <PathPrerequisite       Inverted="true"     Flags="Prerequisite,Discard">ClassColonizedStarSystem/StarSystemImprovementUniqueStrategicVaulters,!FastForwardedImprovement</PathPrerequisite>
        <PathPrerequisite       Inverted="true"     Flags="Prerequisite,Discard">../ClassEmpire/Uniques,StarSystemImprovementUniqueStrategicVaulters</PathPrerequisite>
        <InterpreterPrerequisite Inverted="true"    Flags="Prerequisite,Discard">Path(Context,@'../ClassEmpire/ClassStarSystem,BuildingStarSystemImprovementUniqueStrategicVaulters') and not(Path(Context,@'ClassStarSystem,BuildingStarSystemImprovementUniqueStrategicVaulters'))</InterpreterPrerequisite>
        <TechnologyPrerequisite Flags="Prerequisite,Discard,Technology">TechnologyStrategicResourceGeneration1</TechnologyPrerequisite>
        <DownloadableContentPrerequisite  Flags="Prerequisite,Discard">DLCVaulters</DownloadableContentPrerequisite>
        <SimulationDescriptorReference Name="StarSystemImprovementUniqueStrategicVaulters" />
        <SimulationDescriptorReference Name="StarSystemImprovementUpkeepStage2Extreme" />
        <SimulationDescriptorReference Name="StarSystemImprovementOncePerEmpire" />
        <PopulationEvent Name="PopulationPoliticalEventBuildingConstructionIndustry1"/>
        <UniqueTags>StarSystemImprovementUniqueStrategicVaulters</UniqueTags>
    </StarSystemImprovementDefinition>
    
