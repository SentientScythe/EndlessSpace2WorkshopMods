        <SupervisorGain Name="Dust"       Weight="15"  />
        <SupervisorGain Name="Prestige"   Weight="10"  />
        <SupervisorGain Name="Approval"   Weight="10"  />
        <SupervisorGain Name="Military"   Weight="10"  />
        <AIGain RessourceName="DustGain" />			
