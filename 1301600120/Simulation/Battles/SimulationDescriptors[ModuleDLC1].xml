<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="../../Schemas/Amplitude.Unity.Simulation.SimulationDescriptor.xsd">


    <!--##################
        #### POPQUEST ####
        ##################-->
    <SimulationDescriptor Name="ModuleSupportDustGenerator" Type="ModuleSupportDustGenerator">
        <Property Name="NetShipMoney" BaseValue="0"/>

        <Modifier TargetProperty="NetShipMoney"      Operation="Multiplication" Value="$(Multiplier)" />
        <Modifier TargetProperty="EmpireMoney" Operation="Addition" Value="$(NetShipMoney)" Path="../ClassEmpire"/>
    </SimulationDescriptor>

    <SimulationDescriptor Name="ModuleSupportDustGenerator1Strategic4" Type="ModuleSupportDustGenerator">
        <Modifier TargetProperty="NetShipMoney" Operation="Addition" Value="5" Path="ModuleSupportDustGenerator"/>
    </SimulationDescriptor>
    
    <SimulationDescriptor Name="ModuleSupportStratRecycling" Type="ModuleSupportStratRecycling">
        <Property Name="OpponentRecyclingStrategic1PerCP" BaseValue="0"/>
        <Property Name="OpponentRecyclingStrategic2PerCP" BaseValue="0"/>

        <Modifier TargetProperty="OpponentRecyclingStrategic1PerCP"      Operation="Multiplication" Value="$(Multiplier)" />
        <Modifier TargetProperty="OpponentRecyclingStrategic2PerCP"      Operation="Multiplication" Value="$(Multiplier)" />

        <Modifier TargetProperty="OpponentRecyclingStrategic1PerCP"      Operation="Addition" Value="$(OpponentRecyclingStrategic1PerCP)"       Path="../ClassShip"     Priority="1"/>
        <Modifier TargetProperty="OpponentRecyclingStrategic2PerCP"      Operation="Addition" Value="$(OpponentRecyclingStrategic2PerCP)"       Path="../ClassShip"     Priority="1"/>
    </SimulationDescriptor>
    
    <SimulationDescriptor Name="ModuleSupportStratRecycling1" Type="ModuleSupportStratRecycling">
        <Modifier TargetProperty="OpponentRecyclingStrategic1PerCP" Operation="Addition" Value="1" Path="ModuleSupportStratRecycling"/>
        <Modifier TargetProperty="OpponentRecyclingStrategic2PerCP" Operation="Addition" Value="1" Path="ModuleSupportStratRecycling"/>
    </SimulationDescriptor>

    
    <!--##################
        #### DLC1 ####
        ##################-->
    <SimulationDescriptor Name="ModuleDefenseHullPlatingQuest1"  Type="ModuleDefense">
