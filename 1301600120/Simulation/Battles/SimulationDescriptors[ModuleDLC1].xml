        <Modifier TargetProperty="BoardingPodHealth"        Operation="Addition" Value="250"            Path="ClassModule"      TooltipHidden="true"/>
    </SimulationDescriptor>
    
    <!-- Boarding Pod 2-->
    <SimulationDescriptor Name="ModuleWeaponBoardingPod2Vaulters"  Type="ModuleWeapon">
        <Modifier TargetProperty="ManpowerDamageOverTime"            Operation="Addition" Value="10"        Path="ClassModule"      TooltipHidden="true"/>
        <Modifier TargetProperty="ShipManpowerKillPerHit"            Operation="Addition" Value="15"        Path="ClassModule"      TooltipHidden="true"/>

        <Modifier TargetProperty="BoardingPodHealth"        Operation="Addition" Value="350"            Path="ClassModule"      TooltipHidden="true"/>
    </SimulationDescriptor>

    <!-- Boarding Pod 3-->
    <SimulationDescriptor Name="ModuleWeaponBoardingPod3"  Type="ModuleWeapon">
        <Modifier TargetProperty="ManpowerDamageOverTime"            Operation="Addition" Value="20"     Path="ClassModule"      TooltipHidden="true"/>

        <Modifier TargetProperty="BoardingPodHealth"        Operation="Addition" Value="350"            Path="ClassModule"      TooltipHidden="true"/>
    </SimulationDescriptor>

    <!-- Boarding Defense -->
    <SimulationDescriptor Name="ModuleSupportBoardingDefense" Type="ModuleSupport">
        <Property Name="ManpowerDamageOverTimeDefense" BaseValue="0"/>
        <Property Name="LocalManpower"/>

        <Modifier TargetProperty="ManpowerDamageOverTimeDefense" Operation="Addition" Value="$(ManpowerDamageOverTimeDefense)" Path="../ClassShip" TooltipHidden="true"/>
        <Modifier TargetProperty="MaximumShipManpower"      Operation="Addition"       Value="$(LocalManpower)"         Path="../ClassShip"     Priority="1"/>
    </SimulationDescriptor>

    <SimulationDescriptor Name="ModuleSupportBoardingDefense1" Type="ModuleSupport">
        <Modifier TargetProperty="ManpowerDamageOverTimeDefense" Operation="Addition" Value="0.2" Path="ClassModule" TooltipHidden="true"/>
        <Modifier TargetProperty="LocalManpower"           Operation="Addition" Value="150"    Path="ModuleSupportBoardingDefense"/>
    </SimulationDescriptor>


    <!--################################-->
    <!--###             AOE          ###-->
    <!--################################-->
    <SimulationDescriptor Name="ClassModuleWeaponAOE"  Type="ClassModuleWeapon">
        <!-- Properties -->
        <Property Name="DamageAOE" BaseValue="0"/>
        <Property Name="TargetingWeightBonus"                BaseValue="0"/>
        
        <!-- Modifiers -->
        <Modifier TargetProperty="Cooldown"                     Operation="Addition"        Value="40"       Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="PreparationDuration"          Operation="Addition"        Value="0.5"      Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="PreparationDurationFactorMin" Operation="Multiplication"  Value="0.8"      Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="PreparationDurationFactorMax" Operation="Multiplication"  Value="1.2"      Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="ShootingDuration"             Operation="Addition"        Value="5"        Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="ShotsCountPerSalvo"           Operation="Addition"        Value="1"        Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="ProjectileSpeed"              Operation="Addition"        Value="90"       Path="ClassModuleWeapon" TooltipHidden="true"/>

        <Modifier TargetProperty="CriticalHitChance"            Operation="Addition"        Value="0"        Path="ClassModuleWeapon" />
        <Modifier TargetProperty="ShieldPenetrationFactor"      Operation="Addition"        Value="0.6"      Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="HullPlatingPenetrationFactor" Operation="Addition"        Value="0.1"      Path="ClassModuleWeapon" TooltipHidden="true"/>

        <Modifier TargetProperty="LongRangePenalty"             Operation="Addition"        Value="0"        Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="MediumRangePenalty"           Operation="Addition"        Value="0"        Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="ShortRangePenalty"            Operation="Addition"        Value="0"        Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="DeadZoneRangePenalty"         Operation="Addition"        Value="1"        Path="ClassModuleWeapon" TooltipHidden="true"/>

        <Modifier TargetProperty="DamageAgainstSmallShipsModifier"        Operation="Addition"        Value="1"         Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="DamageAgainstMediumShipsModifier"       Operation="Addition"        Value="0.5"       Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="DamageAgainstLargeShipsModifier"        Operation="Addition"        Value="0.5"       Path="ClassModuleWeapon" TooltipHidden="true"/>

        <!-- We do not feedback DamagePerSecond for AoE weapons, but the explosion damage-->
        <Modifier       TargetProperty="DamagePerSecond"    Operation="Multiplication"    Value="0"              Path="ClassModuleWeapon" Priority="1" TooltipHidden="true"/>
        <BinaryModifier TargetProperty="OffensiveMilitaryPower" Operation="Addition" Left="$(Damage)" BinaryOperation="Division" Right="20" Path="../ClassShip"  TooltipHidden="true"/>

        <Modifier       TargetProperty="DamageAOE"          Operation="Addition"    Value="$(Damage)"              Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="TargetingWeightBonus"      Operation="Addition" Value="$(TargetingWeightBonus)"       Path="../ClassShip"     Priority="1"/>
        
        <!--Every time a module is installed, the upkeep grows-->
        <Modifier TargetProperty="GarrisonMoneyUpkeep"          Operation="Addition" Value="$(UpkeepFromModules)"      Path="../ClassShip"/>
    </SimulationDescriptor>
    
    <!--Aoe weapon-->
