    </SimulationDescriptor>

    <SimulationDescriptor Name="ModuleWeaponMissileQuest01"  Type="ModuleWeapon">
        <Modifier TargetProperty="Damage"                       Operation="Addition" Value="450"     TooltipHidden="true"/>
        <Modifier TargetProperty="CriticalHitChance"            Operation="Addition" Value="0.15"   TooltipHidden="true"/>

        <!-- Must be same value as Salvo Health-->
        <Modifier TargetProperty="MissileHealth"                Operation="Addition" Value="90" TooltipHidden="true"/>
    </SimulationDescriptor>

    <SimulationDescriptor Name="ModuleSupportEngineQuest1" Type="ModuleSupportEngine">
        <Modifier TargetProperty="MaximumMovement"         Operation="Addition"     Value="4"      Path="ModuleSupportEngine"/>
        <Modifier TargetProperty="Evasion"                 Operation="Addition"     Value="0.1"    Path="ModuleSupportEngine"/>
    </SimulationDescriptor>
    
    <SimulationDescriptor Name="ModuleSupportEngine4Vaulters" Type="ModuleSupportEngine">
        <Modifier TargetProperty="MaximumMovement"         Operation="Addition"     Value="5"      Path="ModuleSupportEngine"/>
    </SimulationDescriptor>
    
    <SimulationDescriptor Name="ModuleSupportRepairEngine4Vaulters" Type="ModuleSupportRepair">
        <Modifier TargetProperty="SectionHealthBonus"       Operation="Addition" Value="50"    Path="ClassModule"/>
        <Modifier TargetProperty="RepairAfterBattle" Operation="Addition" Value="0.1" Path="ModuleSupportRepair"/>
        <Modifier TargetProperty="DefensiveMilitaryPower"   Operation="Percent" Value="0.02" Path="../ClassShip"/>
    </SimulationDescriptor>
    
    <SimulationDescriptor Name="ModuleDefenseHullPlating2Vaulters"  Type="ModuleDefense">
        <Modifier TargetProperty="SectionHealthBonus"               Operation="Addition" Value="200"   Path="ClassModuleDefense"/>
        <Modifier TargetProperty="HullPlatingAbsorption"            Operation="Addition" Value="28"   Path="ClassModuleDefense"/>
    </SimulationDescriptor>

    <!--##### DEEDS ####-->
    <SimulationDescriptor Name="ModuleWeaponKineticFlackQuest01"  Type="ModuleWeapon">
        <Modifier TargetProperty="Damage"                    Operation="Addition"  Value="21"    TooltipHidden="true"/>
        <BinaryModifier TargetProperty="DefensiveMilitaryPower"    Operation="Addition" Left="$(FlakDPSMissile)" BinaryOperation="Division" Right="2"      Path="ClassModule" TooltipHidden="true"/>

        <!--DPS against Missile / Squadrons-->
        <Modifier TargetProperty="FlakDamage"           Operation="Addition" Value="25" Path="ClassModule" TooltipHidden="true"/>
        <BinaryModifier TargetProperty="FlakDPSMissile"      Operation="Addition" Left="$(FlakDamage)" BinaryOperation="Division" Right="$(FlakCooldown)"     Path="ClassModule" TooltipHidden="true"/>
        <Modifier TargetProperty="EfficiencyAgainstSquadron" Operation="Addition" Value="0.975"     Path="ClassModule" TooltipHidden="true"/>
        <BinaryModifier TargetProperty="FlakDPSSquadron"      Operation="Addition" Left="$(FlakDPSMissile)" BinaryOperation="Multiplication" Right="$(EfficiencyAgainstSquadron)"     Path="ClassModule" TooltipHidden="true"/>
    </SimulationDescriptor>

    <!--Flack 06 -->
    <SimulationDescriptor Name="ModuleWeaponFlackQuest01"  Type="ModuleWeaponFlack">
        <Modifier TargetProperty="Damage"                       Operation="Addition" Value="25" TooltipHidden="true"/>
        <Modifier TargetProperty="EfficiencyAgainstSquadron" Operation="Addition" Value="0.975"     Path="ClassModule" TooltipHidden="true"/>
    </SimulationDescriptor>

    <SimulationDescriptor Name="ModuleDefenseHullQuest01"  Type="ModuleDefense"> 
        <Property Name="MaximumHealthFleet" BaseValue="0"/>
        <Modifier TargetProperty="MaximumHealthFleet"             Operation="Addition"            Value="0.1"     Path="ClassModule"/>
        <Modifier TargetProperty="SectionHealthBonus"               Operation="Addition" Value="360"  Path="ClassModuleDefense"/>
        <Modifier TargetProperty="HullPlatingAbsorption"            Operation="Addition" Value="53"   Path="ClassModuleDefense"/>
        
        <Modifier TargetProperty="MaximumHealth"             Operation="Percent"            Value="0.1"     Path="../ClassGarrisonFleet/ClassShip,!ShipRoleHero/ClassSection,ClassSectionCore"/>
        <Modifier TargetProperty="MaximumHealth"             Operation="Percent"            Value="0.1"     Path="../ClassShip/ClassSection,ClassSectionCore"/>      
    </SimulationDescriptor>

    <SimulationDescriptor Name="ModuleSupportExperienceQuest01"  Type="ModuleSupport">
        <Property Name="ExperiencePerTurnFleet" BaseValue="0"/>
        <Modifier TargetProperty="ExperiencePerTurnFleet"             Operation="Addition"            Value="2"     Path="ClassModule"/>
        
        <Modifier TargetProperty="ExperiencePerTurn"             Operation="Addition"            Value="2"     Path="../ClassGarrisonFleet/ClassShip"/>
        <Modifier TargetProperty="ExperiencePerTurn"             Operation="Addition"            Value="2"     Path="../ClassGarrisonFleet/ClassHero"/>
    </SimulationDescriptor>

    <SimulationDescriptor Name="ClassModuleSpecialHeroOnly" Type="ClassModuleSpecialHeroOnly"/>
    <SimulationDescriptor Name="ClassModuleShootsOncePerPhase" Type="ClassModuleShootsOncePerPhase"/>
    <SimulationDescriptor Name="ClassModuleIgnoresDefenses" Type="ClassModuleIgnoresDefenses"/>
    <SimulationDescriptor Name="ClassModuleAOETargetsFlotilla" Type="ClassModuleAOETargetsFlotilla"/>

    <!--################################-->
    <!--###         BOARDING         ###-->
    <!--################################-->
    <SimulationDescriptor Name="ClassModuleWeaponBoardingPod"  Type="ClassModuleWeapon">
        <Property Name="ManpowerDamageOverTime"             BaseValue="0"/>
        <Property Name="ManpowerTotalDamageDealt"           BaseValue="0"/>
        <Property Name="LastManpowerDamageOverTime"         BaseValue="0" IsSealed="true"/>
        <Property Name="ManpowerDamageOverTimeDuration"     BaseValue="0"/>
        <Property Name="BoardingPodHealth"                  BaseValue="0"/>
		<Property Name="Dummy"                  			BaseValue="0"/>		

        <Modifier TargetProperty="Cooldown"                     Operation="Addition"        Value="20"       Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="PreparationDuration"          Operation="Addition"        Value="0.5"      Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="PreparationDurationFactorMin" Operation="Multiplication"  Value="0.8"      Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="PreparationDurationFactorMax" Operation="Multiplication"  Value="1.2"      Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="ShootingDuration"             Operation="Addition"        Value="2"        Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="ShotsCountPerSalvo"           Operation="Addition"        Value="1"        Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="ProjectileSpeed"              Operation="Addition"        Value="50"       Path="ClassModuleWeapon" TooltipHidden="true"/>

        <Modifier TargetProperty="CriticalHitChance"            Operation="Addition"        Value="0"        Path="ClassModuleWeapon" />
        <Modifier TargetProperty="ShieldPenetrationFactor"      Operation="Addition"        Value="0.6"      Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="HullPlatingPenetrationFactor" Operation="Addition"        Value="0.1"      Path="ClassModuleWeapon" TooltipHidden="true"/>

        <Modifier TargetProperty="LongRangePenalty"             Operation="Addition"        Value="0"        Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="MediumRangePenalty"           Operation="Addition"        Value="0"        Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="ShortRangePenalty"            Operation="Addition"        Value="0"        Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="DeadZoneRangePenalty"         Operation="Addition"        Value="1"        Path="ClassModuleWeapon" TooltipHidden="true"/>

        <!--Boarding specifics-->
        <Modifier TargetProperty="ManpowerDamageOverTimeDuration"         Operation="Addition"        Value="10"                 Path="ClassModuleWeapon" TooltipHidden="true"/>
        <Modifier TargetProperty="ManpowerDamageOverTime"                 Operation="Multiplication"  Value="$(Multiplier)"     Path="ClassModuleWeapon" TooltipHidden="true"/>
		
		<Modifier TargetProperty="Dummy" Operation="Addition" Value="$(ProjectileSpeed)" Path="ClassModuleWeapon"/>		
        
        <Modifier TargetProperty="BoardingPodHealth"                 Operation="Multiplication"  Value="$(Multiplier)"     Path="ClassModuleWeapon" TooltipHidden="true"/>
        
        <BinaryModifier TargetProperty="ManpowerTotalDamageDealt" Operation="Addition" Left="$(ManpowerDamageOverTime)" BinaryOperation="Multiplication" Right="$(ManpowerDamageOverTimeDuration)" Path="ClassModuleWeapon" TooltipHidden="true"/>

        <BinaryModifier TargetProperty="OffensiveMilitaryPower" Operation="Addition" Left="$(ManpowerTotalDamageDealt)" BinaryOperation="Multiplication" Right="5" Path="../ClassShip" TooltipHidden="true"/>

        <Modifier TargetProperty="ManpowerDamageOverTime"                 Operation="Addition"  Value="$(ManpowerDamageOverTime)"     Path="../ClassShip" TooltipHidden="true"/>
        <!--Every time a module is installed, the upkeep grows-->
        <Modifier TargetProperty="GarrisonMoneyUpkeep"          Operation="Addition" Value="$(UpkeepFromModules)"      Path="../ClassShip"/>
    </SimulationDescriptor>

    <!--Boarding Pod 1-->
    <SimulationDescriptor Name="ModuleWeaponBoardingPod1"  Type="ModuleWeapon">
        <Modifier TargetProperty="ManpowerDamageOverTime"            Operation="Addition" Value="5"     Path="ClassModule"      TooltipHidden="true"/>

        <Modifier TargetProperty="BoardingPodHealth"        Operation="Addition" Value="200"            Path="ClassModule"      TooltipHidden="true"/>
    </SimulationDescriptor>

    <!-- Boarding Pod 2-->
